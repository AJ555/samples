{
  "lambda": {
    "FunctionName": "box-node-rekognition-webhook-lambda-sample",
    "Handler": "index.handler",
    "Runtime": "nodejs4.3",
    "Description": "Demonstrates connecting a Box webhook to an AWS Lambda function via API Gateway. The lambda function stores the event in DynamoDB",
    "Environment": {
      "Variables": {
        "BOX_WEBHOOK_PRIMARY_SIGNATURE_KEY": "SamplePrimaryKey",
        "BOX_WEBHOOK_SECONDARY_SIGNATURE_KEY": "SampleSecondaryKey"
      }
    }
  },
  "triggers": {
    "api-gateway": [
      {}
    ]
  },
  "version": "1.0.0",
  "license": "Apache-2.0",
  "tags": [
    "nodejs",
    "box",
    "webhook",
    "ext-libraries"
  ],
